        <item name="android:paddingBottom">@dimen/margin_5</item>
        <item name="android:textColor">@color/black_66</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textSize">@dimen/textsize_15</item>
    </style>
    <style name="text_makeractivity_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/height_40</item>
        <item name="android:textColor">@color/black_66</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_gravity">center_vertical|right</item>
