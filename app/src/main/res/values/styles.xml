        <item name="android:textSize">@dimen/textsize_14</item>
    </style>
    <style name="text_invoicespecialauditing_right">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/margin_5</item>
        <item name="android:paddingBottom">@dimen/margin_5</item>
        <item name="android:textColor">@color/black_66</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/textsize_15</item>
    </style>

    <style name="text_invoicespecialaudit_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textColor">@color/black_99</item>
        <item name="android:layout_marginTop">@dimen/margin_10</item>
