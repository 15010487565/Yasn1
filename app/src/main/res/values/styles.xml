        <item name="android:layout_height">@dimen/height_40</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/black_66</item>
        <item name="android:textSize">@dimen/textsize_16</item>
        <item name="android:background">@drawable/shape_black_white</item>
        <item name="android:layout_marginTop">@dimen/margin_20</item>
        <item name="android:paddingLeft">@dimen/margin_20</item>
    </style>

    <style name="oil_query_result">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginTop">@dimen/margin_5</item>
        <item name="android:gravity">center_vertical</item>
    </style>
</resources>
